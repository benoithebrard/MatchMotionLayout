<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_overall">

    <fragment
        android:id="@+id/navigation_overall"
        android:name="com.example.matchmotionlayout.ui.fragment.OverallMatchFragment"
        android:label="@string/title_football"
        tools:layout="@layout/fragment_match" />

    <fragment
        android:id="@+id/navigation_overall_no_icon"
        android:name="com.example.matchmotionlayout.ui.fragment.OverallNoIconMatchFragment"
        android:label="@string/title_football"
        tools:layout="@layout/fragment_match" />

    <fragment
        android:id="@+id/navigation_overall_legacy"
        android:name="com.example.matchmotionlayout.ui.fragment.OverallLegacyMatchFragment"
        android:label="@string/title_team"
        tools:layout="@layout/fragment_match" />

    <fragment
        android:id="@+id/navigation_periods"
        android:name="com.example.matchmotionlayout.ui.fragment.PeriodsMatchFragment"
        android:label="@string/title_tennis"
        tools:layout="@layout/fragment_match" />

</navigation>